class Solution:
    def removeDuplicateLetters(self, s: str) -> str:
        rindex = {ch: i for i, ch in enumerate(s)}
        print(rindex)
        res = []
        for i,ch in enumerate(s):
            if ch not in res:
                while (res and ch< res[-1] and i< rindex[res[-1]]):
                    res.pop()
                res.append(ch)
        return ''.join(res)
